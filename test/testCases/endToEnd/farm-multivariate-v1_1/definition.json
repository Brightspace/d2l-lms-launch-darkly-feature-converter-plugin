					"variation": "grapes"
				}
			]
		},
		"development": {
			"defaultVariation": "grapes",
			"rules": [
				{
					"versions": {
						"start": "10.8.4.0"
					},
					"variation": "none"
				},
				{
					"dataCenters": [
						"us04"
					],
